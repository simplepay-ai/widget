{
    "useTabs": false,
    "tabWidth": 4,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 100,
    "plugins": ["@ianvs/prettier-plugin-sort-imports"],
    "importOrder": ["<TYPES>", "<BUILTIN_MODULES>", "<THIRD_PARTY_MODULES>", "^[.]"],
    "importOrderParserPlugins": ["decorators", "typescript"]
}
